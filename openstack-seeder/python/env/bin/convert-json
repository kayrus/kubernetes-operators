#!/Users/d058695/Development/go/src/github.com/sapcc/kubernetes-operators/openstack-seeder/python/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'oslo.log==4.1.1','console_scripts','convert-json'
__requires__ = 'oslo.log==4.1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('oslo.log==4.1.1', 'console_scripts', 'convert-json')()
    )
