#!/Users/d058695/Development/go/src/github.com/sapcc/kubernetes-operators/openstack-seeder/python/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'python-novaclient==14.2.0','console_scripts','nova'
__requires__ = 'python-novaclient==14.2.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('python-novaclient==14.2.0', 'console_scripts', 'nova')()
    )
